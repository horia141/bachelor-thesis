Name:     Auto2Rom
AddrSize: 3
WordSize: 20
Format:   Bin

# alu is device 0 : 000
# swc is device 1 : 001
# alu:Result is input 0 : 00
# swc:Ready is input 1  : 01
# @start is 0 : 0000_0000

0001_0000_0001_0001_1010 # @start ci alu:ldi 1Ah
0001_0000_0010_0000_0010 # 	  ci alu:add 02h
0010_0001_0001_0000_0000 #	  cr swc:ld0 alu:Result
0001_0001_0111_0000_0000 # 	  ci swc:ccd
0101_0000_0100_0000_0001 # @loop  jz @loop swc:Ready
0011_0000_0000_0000_0000 #	  ji @start
